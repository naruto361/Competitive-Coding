#include <bits/stdc++.h>
using namespace std;
int prime(int x)
{	
	if(x>=94 && x<=130)
	{
		if(x<=99) return 97;
	else if(x<=102) return 101;
	else if(x<=105) return 103;
	else if(x<=108) return 107;
	else if(x<=111) return 109;
	else return 113;
	}
	else
	{
	if(x<=69) return 67;
	else if(x<=72) return 71;
	else if(x<=76) return 73;
	else if(x<=81) return 79;
	else if(x<=86) return 83;
	else if(x<=93) return 89;
	}
}
int main() 
{
	int t;
	cin>>t;
	while(t--)
	{
		int n;
		cin>> n;
		string s;
		cin>>s;
		for(int i=0;i<n;i++)
		{
			cout<<char(prime(int(s[i])));
		}
		cout<<endl;

	}
}

