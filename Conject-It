

#include <iostream>

using namespace std;

int main()
{
	int n;
	cin >>n;
	
	while(n--)

	{   
		int j=0;unsigned long long int t;
	    cin>>t;
		int x=10000;
	    while(x--)
		{
	    if(t%2==0)
	    t=t/2;
	    else
	    t=(3*t)+1;
	    if(t==1)
	    {j=1;break;}
		}

	    if(j==1)
		cout<<"YES";
		else
	    cout<<"NO";
		cout<<endl;
	}
}
